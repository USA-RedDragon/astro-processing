{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview --host",
    "generate:graphql": "graphql-codegen --config codegen.ts",
    "test:e2e": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed'",
    "test:e2e:electron": "npm run test:e2e",
    "test:e2e:chrome": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed --browser chrome'",
    "test:e2e:firefox": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed --browser firefox'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' localhost:4173 'cypress open --e2e'",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "lint": "eslint src/ tests/",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.17",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "14.1.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.5",
    "lucide-vue-next": "0.555.0",
    "mitt": "3.0.1",
    "reka-ui": "2.6.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.17",
    "tw-animate-css": "1.4.0",
    "vue": "3.5.25",
    "vue-router": "4.6.3"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.15.1",
    "@eslint/js": "^9.39.1",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/client-preset": "^5.2.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-operations": "^5.0.5",
    "@iconify-json/radix-icons": "1.2.5",
    "@iconify/vue": "5.0.0",
    "@rushstack/eslint-patch": "1.15.0",
    "@tanstack/vue-table": "8.21.3",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "6.0.2",
    "@vitest/eslint-plugin": "1.5.0",
    "@vitest/ui": "4.0.14",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "15.7.0",
    "cypress-mochawesome-reporter": "4.0.2",
    "cypress-multi-reporters": "2.0.5",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^10.6.1",
    "jsdom": "^27.2.0",
    "mocha-junit-reporter": "2.2.1",
    "mochawesome": "7.1.4",
    "mochawesome-merge": "5.1.0",
    "mochawesome-report-generator": "6.3.2",
    "prettier": "3.6.2",
    "sass": "1.95.0",
    "start-server-and-test": "2.1.3",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "7.2.4",
    "vitest": "4.0.14",
    "vue-tsc": "3.1.5"
  }
}
